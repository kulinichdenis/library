{{#link-to 'books.new' }}<a href="">New</a>{{/link-to}}
<h1>Books</h1>
<table class="table table-hover">
	<thead>
		<tr>
			<th>Book</th>
			<th>Library</th>
		</tr>
	</thead>
	<tbody>
		{{#each model as |book|}}
			<tr>
				<td>{{ book.title }}</td>
				<td>{{ book.library.name }}</td>
			</tr>
		{{/each}}
	</tbody>
</table>

<nav>
  <ul class="pager">
		{{#link-to (query-params  page=prevPage) tagName='li' disabled=prevButton	}}<a href="">Previous</a>{{/link-to}}
		{{#link-to (query-params page=nextPage) tagName='li'  disabled=nextButton }}<a href="">Next</a>{{/link-to}}
  </ul>
</nav>
