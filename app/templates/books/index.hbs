<h1>test</h1>
<table class="table table-bordered table-striped">
  <thead>
  <tr>
    <th>
      Book
    </th>
    <th class="vtop">Author</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  {{#each model as |book|}}
    <tr>
      <td>
        {{ book.title }}
      </td>
      <td>
        {{book.author.name }}
      </td>
      <td class="text-center">
        <button class="btn btn-success" {{action 'deleteBook' book}}>Delete</button>
      </td>
    </tr>
  {{/each}}
  </tbody>
</table>
